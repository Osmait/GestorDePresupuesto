{
  "extends": "next/core-web-vitals",
  "ignorePatterns": [
    "components/ui/**"
  ],
  "rules": {
    "no-unused-vars": [
      "warn",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_"
      }
    ],
    "no-unused-expressions": "warn",
    "no-unreachable": "error",
    "no-empty": "warn",
    "no-useless-return": "warn",
    "react-hooks/exhaustive-deps": "warn",
    "react-hooks/rules-of-hooks": "error"
  }
}